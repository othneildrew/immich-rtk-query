{
  "name": "immich-rtk-query",
  "version": "2.4.1",
  "description": "Type-safe RTK Query API client for Immich photo management platform",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "generate": "rtk-query-codegen-openapi openapi-config.cjs",
    "prepare": "npm run generate && npm run build",
    "clean": "rm -rf dist",
    "test": "echo \"Note: no test specified. Passing...\" && exit 0"
  },
  "publishConfig": {
    "@othneildrew:registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "immich",
    "rtk-query",
    "redux",
    "api",
    "typescript",
    "photo-management"
  ],
  "author": "Othneil Drew",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/othneildrew/immich-rtk-query.git"
  },
  "homepage": "https://github.com/othneildrew/immich-rtk-query.git#readme",
  "bugs": {
    "url": "https://github.com/othneildrew/immich-rtk-query/issues"
  },
  "type": "module",
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "react": "^18.0.0 || ^19.0.0",
    "react-redux": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@rtk-query/codegen-openapi": "^2.2.0",
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
